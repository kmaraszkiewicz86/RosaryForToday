<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="RosaryForToday.UI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:RosaryForToday.Presentation.Views;assembly=RosaryForToday.Presentation"
    Title="RosaryForToday.UI">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate views:RosaryListView}"
        Route="MainPage" />

    <ShellContent
        Title="Crash log list"
        ContentTemplate="{DataTemplate views:CrashLogView}"
        Route="CrashLog" />

</Shell>
